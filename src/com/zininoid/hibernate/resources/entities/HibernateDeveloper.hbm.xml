<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping
		PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zininoid.hibernate.entities.HibernateDeveloper" table="HIBERNATE_DEVELOPERS">

		<id name="id" type="int" column="id">
			<generator class="native"/>
		</id>

		<set name="projects" cascade="all" table="HIBERNATE_DEV_PROJECTS">
			<key column="DEVELOPER_ID" />
			<many-to-many column="PROJECT_ID" class="com.zininoid.hibernate.entities.HibernateProject"/>
		</set>

		<property name="firstName" column="FIRST_NAME" type="string"/>
		<property name="lastName" column="LAST_NAME" type="string"/>
		<property name="specialty" column="SPECIALTY" type="string"/>
		<property name="experience" column="EXPERIENCE" type="int"/>

	</class>

</hibernate-mapping>