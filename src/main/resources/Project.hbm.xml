<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entities.Project" table="C_PROJECT">

        <id name="id" type="long" column="id">
            <generator class="native"/>
        </id>

        <property name="projectName" column="PROJECT_NAME" type="string"/>
        <property name="from" column="FROM" type="java.util.Calendar"/>
        <property name="to" column="TO" type="java.util.Calendar"/>

    </class>
</hibernate-mapping>